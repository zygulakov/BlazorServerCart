@page "/{page}/{id?}"
@inject NavigationManager NavigationManager

<h1>User Id: @Id</h1>

@code {
    [Parameter]
    public string Id { get; set; }
    [Parameter]
    public string Page { get; set; }

    protected override void OnParametersSet()
    {
        base.OnParametersSet();
        NavigationManager.NavigateTo($"stacklayout");
    }
}